@{
    ViewBag.Title = "Святопетрівьска сільска рада";
}

<div class="big-wrapper">
    <div class="wrapper">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-12">
                    <section class="heading-slider" data-interval="6000">
                        <div class="heading-slider-single-slide">
                            <img src="~/_assets/_img/image.png" class="image-data-for-parent" alt="Living room with a perfect view" />
                            <div class="heading-slider-single-slide-content">                                
                                <h1>Святопетрівьска Сільська рада</h1>
                                <h3>Офіційний портал</h3>
                            </div>
